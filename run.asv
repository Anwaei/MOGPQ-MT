% Toy
% y1 = x1cos(x2)
% y2 = x1sin(x2)
% SE kernel, alpha = 1, l = [60 6]

clear;

func_g = @(x) [x(1)*cos(x(2));x(1)*sin(x(2))];

conf.D = 2;
conf.Q = 2;

m = [4; 45/180*pi];
P = [0.5, 0; 0, 6/180*pi];
func_pdf = @(x) 1/

% Sigma points
[Xi_s, N] = getSigmaPoints(conf.D);
conf.N = N;

% Kernel hypers
conf.cov = @covSEard;
alpha = 1; l = [6 3];
hyp = [log(l) log(alpha)];

[Mu, Pi, C] = GPQMT(m, P, hyp, Xi_s, func_g, conf);
